{
	"name": "latest-demo",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite --host",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview --port 5050",
		"lint": "prettier ./src/ --write",
		"test:unit": "vitest --environment jsdom",
		"typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"prepare": "husky install"
	},
	"dependencies": {
		"axios": "^0.25.0",
		"lodash-es": "^4.17.21",
		"pinia": "^2.0.13",
		"swiper": "^8.3.1",
		"view-ui-plus": "^1.3.1",
		"vue": "^3.2.33",
		"vue-router": "^4.0.14"
	},
	"devDependencies": {
		"@types/jsdom": "^16.2.14",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^16.11.27",
		"@vitejs/plugin-vue": "^2.3.1",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"@vue/test-utils": "^2.0.0-rc.20",
		"@vue/tsconfig": "^0.1.3",
		"autoprefixer": "^10.4.7",
		"husky": "^8.0.1",
		"jsdom": "^19.0.0",
		"less": "^4.1.3",
		"less-loader": "^11.0.0",
		"lint-staged": "^13.0.3",
		"postcss": "^8.4.13",
		"prettier": "^2.5.1",
		"prettier-plugin-tailwindcss": "^0.1.10",
		"scp2": "^0.5.0",
		"tailwindcss": "^3.0.24",
		"typescript": "~4.6.3",
		"vite": "^2.9.5",
		"vitest": "^0.9.3",
		"vue-tsc": "^0.34.7"
	},
	"lint-staged": {
		"*.{js,css,md}": "prettier --write"
	}
}
